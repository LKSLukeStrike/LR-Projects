//region LR_Elems Cfk Dotted Dotter
function getElemDottedDotter(elem) {
	debugFA("getElemDottedDotter", arguments)
	const result = Not_Dotter
	Object.keys(Key_DOTDOR).forEach(function(cfk_dottednn) {
