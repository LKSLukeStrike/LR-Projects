	debugFA("setElemDottedDotter", arguments)
	if (yesArray(elem)) {return elem.forEach(function(elem) { // recursive
		setElemDottedDotter(elem, arr_dotternn)
	})} // single elem from here
	const arr_dotternn = getElemDottedDotter(elem)
