
function computeTotCharacMax(sheet) { // compute the charac max total
	const tot_characmax = getElem(sheet, Tot_CharacMax)
	
	let result = 0
	
	Arr_CharacsReps.forEach(function(rep_item) { // compute all charac max pts
		const rep_elem   = getElem(sheet, rep_item)

		eachRepItemDo(rep_elem, Max_CharacPts, function(max_characpts) {
			result += getElemValue(max_characpts)
		})
	})
	
	return setElemValue(tot_characmax, result) // set charac max total
}
